{
	"name": "カリキュラムオートセーブ",
	"manifest_version": 3,
	"version": "1.0",
    "description": "自動保存されないことにカッとなって作り申し上げた",
	"permissions": ["scripting", "tabs","idle"],
	"host_permissions":["<all_urls>"], 
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts":[
		{
			"matches": ["<all_urls>"],
			"js": ["scripts/addAutoToHeight.js"]
		}
	],
	"action": {}
}
